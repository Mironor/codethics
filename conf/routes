# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET         /                       controllers.Application.index
POST        /submit                 controllers.Application.submit
GET         /t/:token               controllers.Application.token(token: String)
GET         /done/:token            controllers.Application.tokenDone(token: String)
GET         /vote-one/:token        controllers.Application.voteSnippetOne(token: String)
GET         /vote-two/:token        controllers.Application.voteSnippetTwo(token: String)

# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file           controllers.Assets.at(path="/public", file)
